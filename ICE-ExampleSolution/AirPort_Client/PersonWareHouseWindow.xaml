<local:PopupWindowBase x:Class="AirPort.Client.PersonWareHouseWindow"
                       xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                       xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                       xmlns:local="clr-namespace:AirPort.Client"
                       xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                       Title="PersonWareHouseWindow"
                       d:DesignHeight="600"
                       d:DesignWidth="800"
                       mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" />
            <RowDefinition />
        </Grid.RowDefinitions>

        <StackPanel Orientation="Horizontal">
            <Button Click="Button_Click" Content="人像入库" />

            <Button Click="btnDeletePerson_Click" Content="删除(单个)" />
            <Button Click="btnBatchDelete_Click" Content="删除(批量)" />
            <Button Click="btnUpdateTag_Click" Content="更新标签" />
            <Button Click="btnDeleteTag_Click" Content="删除标签" />
        </StackPanel>
    </Grid>
</local:PopupWindowBase>
